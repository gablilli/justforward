defaults
auth           on
tls            on
tls_starttls   on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        /var/log/msmtp.log
timeout        30

account gmail
host smtp.gmail.com
port 587
from youruser@gmail.com
user youruser@gmail.com
password apppassword

account default : gmail
